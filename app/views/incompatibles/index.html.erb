<p id="notice"><%= notice %></p>

<h1>Listing Incompatibles</h1>

<table>
  <thead>
    <tr>
      <th>Mate</th>
      <th>Incompatibility</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incompatibles.each do |incompatible| %>
      <tr>
        <td><%= incompatible.mate_id %></td>
        <td><%= incompatible.incompatibility_id %></td>
        <td><%= link_to 'Show', incompatible %></td>
        <td><%= link_to 'Edit', edit_incompatible_path(incompatible) %></td>
        <td><%= link_to 'Destroy', incompatible, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incompatible', new_incompatible_path %>
